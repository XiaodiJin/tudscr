% respektive \cs{tud@font@init@nfss} bei \cs{tud@font@init}.
%    \begin{macrocode}
  \if@tud@fontspec%
    \preto\tud@font@init\tud@font@init@fontspec%
  \else%
    \preto\tud@font@init\tud@font@init@nfss%
  \fi%
