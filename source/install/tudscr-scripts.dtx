        "Schreibrechte setzen (empfohlen)")
          looptexdirarray use sudo chmod -R o+w
          sudo -k
          texdirpermitted=false
          looptexdirarray texdirpermitted=true
          if $texdirpermitted ; then
            echo
            echo Setzen der Schreibrechte gescheitert
            abort
          fi
          break;;
        "'updmap' verwenden")
          updmapsys=false
          break;;
