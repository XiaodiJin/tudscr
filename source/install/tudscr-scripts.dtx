  )
  looptexdirarray()
  {
    for d in "${texdirarray[@]}"; do
      if [ -d "$d" ] && [ ! -w "$d" ] ; then
        if [ "$1" == "use" ] ; then
          eval ${@:2} $d;
        else
          eval $@;
        fi
      fi
    done
  }
  texdirpermitted=false
  looptexdirarray texdirpermitted=true
  if $texdirpermitted ; then
    echo Fuer das systemweite Registrieren der Schriften ist das Ausfuehren
