  \patchcmd{\ps@tudposter}%
    {\textbf{\@einrichtung}}%
    {\textbf{\fontseries{b}\selectfont\@einrichtung}}%
    {}{\tud@patch@wrn{ps@tudposter}}%
  \pagestyle{tudposter}%
}
